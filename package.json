{
  "name": "markmetcalfe.io",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test": "npx playwright test",
    "preview": "vite preview",
    "precommit": "lint-staged",
    "check-prettier": "prettier . --check && eslint . --max-warnings=0 --ext .js,.ts,.jsx,.tsx --ignore-path=.prettierignore",
    "check-eslint": "eslint . --max-warnings=0 --ext .js,.ts,.jsx,.tsx --ignore-path=.prettierignore",
    "check-typescript": "vue-tsc --noEmit",
    "fix-prettier": "prettier . --write",
    "fix-eslint": "eslint . --max-warnings=0 --ext .js,.ts,.jsx,.tsx --ignore-path=.prettierignore --fix",
    "check": "npm-run-all check-prettier check-eslint check-typescript",
    "fix": "npm-run-all fix-prettier fix-eslint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "is-mobile": "^4.0.0",
    "pinia": "^2.1.7",
    "three": "^0.165.0",
    "vue": "^3.4.21",
    "vuetify": "^3.4.9"
  },
  "devDependencies": {
    "@babel/types": "^7.24.0",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@playwright/test": "^1.42.1",
    "@types/node": "^20.11.24",
    "@types/three": "^0.165.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.22.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "npm-run-all2": "^5.0.0",
    "postcss": "^8.4.35",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.5",
    "vite-plugin-eslint": "^1.8.1",
    "vue-router": "^4.3.0",
    "vue-tsc": "^1.8.27"
  },
  "packageManager": "pnpm@8.15.8"
}
