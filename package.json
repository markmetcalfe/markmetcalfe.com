{
  "name": "markmetcalfe.io",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test": "npx playwright test",
    "preview": "vite preview",
    "precommit": "lint-staged",
    "check-prettier": "prettier . --check && eslint . --max-warnings=0 --ext .js,.ts,.jsx,.tsx --ignore-path=.prettierignore",
    "check-eslint": "eslint . --max-warnings=0 --ext .js,.ts,.jsx,.tsx --ignore-path=.prettierignore",
    "check-typescript": "vue-tsc --noEmit",
    "fix-prettier": "prettier . --write",
    "fix-eslint": "eslint . --max-warnings=0 --ext .js,.ts,.jsx,.tsx --ignore-path=.prettierignore --fix",
    "check": "npm-run-all check-prettier check-eslint check-typescript",
    "fix": "npm-run-all fix-prettier fix-eslint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-regular-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@fortawesome/vue-fontawesome": "3.0.5",
    "is-mobile": "^4.0.0",
    "pinia": "^2.1.7",
    "three": "0.157.0",
    "vue": "3.3.4",
    "vuetify": "^3.4.9"
  },
  "devDependencies": {
    "@babel/types": "7.23.0",
    "@playwright/test": "1.38.1",
    "@types/node": "20.11.0",
    "@types/three": "0.156.0",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "@vitejs/plugin-vue": "5.0.3",
    "@vue/eslint-config-typescript": "12.0.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-vue": "9.17.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.31",
    "postcss-html": "1.5.0",
    "prettier": "3.0.3",
    "sass": "1.69.1",
    "stylelint": "16.1.0",
    "stylelint-config-recommended-scss": "14.0.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard-scss": "13.0.0",
    "typescript": "5.2.2",
    "vite": "5.0.11",
    "vite-plugin-eslint": "1.8.1",
    "vue-router": "4.2.5",
    "vue-tsc": "1.8.18"
  },
  "packageManager": "pnpm@8.9.0"
}
